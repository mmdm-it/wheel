  "verse_count": 22,
  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[LAME 3:1]",
      "word_count": 5,
      "hebrew": "אֲנִ֤י הַגֶּ֨בֶר֙ רָאָ֣ה עֳנִ֔י בְּשֵׁ֖בֶט עֶבְרָתֽוֹ׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[LAME 3:2]",
      "word_count": 5,
      "hebrew": "אוֹתִ֥י נָהַ֛ג וַיֹּלַ֖ךְ חֹ֥שֶׁךְ וְלֹא אֽוֹר׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[LAME 3:3]",
      "word_count": 5,
      "hebrew": "אַ֣ךְ בִּ֥י יָשֻׁ֛ב יַהֲפֹ֥ךְ יָד֖וֹ כָּל הַיּֽוֹם׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[LAME 3:4]",
      "word_count": 5,
      "hebrew": "בִּלָּ֤ה בְשָׂרִי֙ וְעוֹרִ֔י שִׁבַּ֖ר עַצְמוֹתָֽי׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[LAME 3:5]",
      "word_count": 5,
      "hebrew": "בָּנָ֥ה עָלַ֛י וַיַּקַּ֖ף רֹ֥אשׁ וּתְלָאָֽה׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[LAME 3:6]",
      "word_count": 5,
      "hebrew": "בְּמַחֲשַׁכִּ֥ים הוֹשִׁיבַ֖נִי כְּמֵתֵ֥י עוֹלָֽם׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[LAME 3:7]",
      "word_count": 5,
      "hebrew": "גָּדַ֧ר בַּעֲדִ֛י וְלֹ֥א אֵצֵ֖א הִכְבִּ֥יד נְחָשְׁתִּֽי׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[LAME 3:8]",
      "word_count": 5,
      "hebrew": "גַּ֣ם כִּ֤י אֶזְעַק֙ וַאֲשַׁוֵּ֔עַ שָׂתַ֖ם תְּפִלָּתִֽי׃"
    },
    "9": {
      "verse_number": 9,
      "sort_number": 9,
      "text": "[LAME 3:9]",
      "word_count": 5,
      "hebrew": "גָּדַ֤ר דְּרָכַי֙ בְּגָזִ֔ית נְתִיבֹתַ֖י עִוָּֽה׃"
    },
    "10": {
      "verse_number": 10,
      "sort_number": 10,
      "text": "[LAME 3:10]",
      "word_count": 5,
      "hebrew": "דֹּ֣ב אֹרֵ֥ב הוּא֙ לִ֔י אריה אֲרִ֖י בְּמִסְתָּרִֽים׃"
    },
    "11": {
      "verse_number": 11,
      "sort_number": 11,
      "text": "[LAME 3:11]",
      "word_count": 5,
      "hebrew": "דְּרָכַ֥י סוֹרֵ֛ר וַֽיְפַשְּׁחֵ֖נִי שָׂמַ֥נִי שֹׁמֵֽם׃"
    },
    "12": {
      "verse_number": 12,
      "sort_number": 12,
      "text": "[LAME 3:12]",
      "word_count": 5,
      "hebrew": "דָּרַ֤ךְ קַשְׁתוֹ֙ וַיַּצִּיבֵ֔נִי כַּמַּטָּרָ֖א לַחֵֽץ׃"
    },
    "13": {
      "verse_number": 13,
      "sort_number": 13,
      "text": "[LAME 3:13]",
      "word_count": 5,
      "hebrew": "הֵבִיא֙ בְּכִלְיוֹתָ֔י בְּנֵ֖י אַשְׁפָּתֽוֹ׃"
    },
    "14": {
      "verse_number": 14,
      "sort_number": 14,
      "text": "[LAME 3:14]",
      "word_count": 5,
      "hebrew": "הָיִ֤יתִי שְּׂחֹק֙ לְכָל עַמִּ֔י נְגִינָתָ֖ם כָּל הַיּֽוֹם׃"
    },
    "15": {
      "verse_number": 15,
      "sort_number": 15,
      "text": "[LAME 3:15]",
      "word_count": 5,
      "hebrew": "הִשְׂבִּיעַ֥נִי בַמְּרוֹרִ֖ים הִרְוַ֥נִי לַעֲנָֽה׃"
    },
    "16": {
      "verse_number": 16,
      "sort_number": 16,
      "text": "[LAME 3:16]",
      "word_count": 5,
      "hebrew": "וַיַּגְרֵ֤ס בֶּֽחָצָץ֙ שִׁנָּ֔י הִכְפִּישַׁ֖נִי בָּאֵֽפֶר׃"
    },
    "17": {
      "verse_number": 17,
      "sort_number": 17,
      "text": "[LAME 3:17]",
      "word_count": 5,
      "hebrew": "וַתִּזְנַ֧ח מִשָּׁל֛וֹם נַפְשִׁ֖י נָשִׁ֥יתִי טוֹבָֽה׃"
    },
    "18": {
      "verse_number": 18,
      "sort_number": 18,
      "text": "[LAME 3:18]",
      "word_count": 5,
      "hebrew": "וָאֹמַר֙ אָבַ֣ד נִצְחִ֔י וְתוֹחַלְתִּ֖י מֵיְהוָֽה׃"
    },
    "19": {
      "verse_number": 19,
      "sort_number": 19,
      "text": "[LAME 3:19]",
      "word_count": 5,
      "hebrew": "זְכָר עָנְיִ֥י וּמְרוּדִ֖י לַעֲנָ֥ה וָרֹֽאשׁ׃"
    },
    "20": {
      "verse_number": 20,
      "sort_number": 20,
      "text": "[LAME 3:20]",
      "word_count": 5,
      "hebrew": "זָכ֣וֹר תִּזְכּ֔וֹר ותשיח וְתָשׁ֥וֹחַ עָלַ֖י נַפְשִֽׁי׃"
    },
    "21": {
      "verse_number": 21,
      "sort_number": 21,
      "text": "[LAME 3:21]",
      "word_count": 5,
      "hebrew": "זֹ֛את אָשִׁ֥יב אֶל לִבִּ֖י עַל כֵּ֥ן אוֹחִֽיל׃"
    },
    "22": {
      "verse_number": 22,
      "sort_number": 22,
      "text": "[LAME 3:22]",
      "word_count": 5,
      "hebrew": "חַֽסְדֵ֤י יְהוָה֙ כִּ֣י לֹא תָ֔מְנוּ כִּ֥י לֹא כָל֖וּ רַחֲמָֽיו׃"
    }
  }
}