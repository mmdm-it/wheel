  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[PSAL 130:1]",
      "word_count": 5,
      "hebrew": "שִׁ֥יר הַֽמַּעֲל֑וֹת מִמַּעֲמַקִּ֖ים קְרָאתִ֣יךָ יְהוָֽה׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[PSAL 130:2]",
      "word_count": 5,
      "hebrew": "אֲדֹנָי֮ שִׁמְעָ֪ה בְק֫וֹלִ֥י תִּהְיֶ֣ינָה אָ֭זְנֶיךָ קַשֻּׁב֑וֹת לְ֝ק֗וֹל תַּחֲנוּנָֽי׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[PSAL 130:3]",
      "word_count": 5,
      "hebrew": "אִם עֲוֺנ֥וֹת תִּשְׁמָר יָ֑הּ אֲ֝דֹנָ֗י מִ֣י יַעֲמֹֽד׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[PSAL 130:4]",
      "word_count": 5,
      "hebrew": "כִּֽי עִמְּךָ֥ הַסְּלִיחָ֑ה לְ֝מַ֗עַן תִּוָּרֵֽא׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[PSAL 130:5]",
      "word_count": 5,
      "hebrew": "קִוִּ֣יתִי יְ֭הוָה קִוְּתָ֣ה נַפְשִׁ֑י וְֽלִדְבָר֥וֹ הוֹחָֽלְתִּי׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[PSAL 130:6]",
      "word_count": 5,
      "hebrew": "נַפְשִׁ֥י לַֽאדֹנָ֑י מִשֹּׁמְרִ֥ים לַ֝בֹּ֗קֶר שֹׁמְרִ֥ים לַבֹּֽקֶר׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[PSAL 130:7]",
      "word_count": 5,
      "hebrew": "יַחֵ֥ל יִשְׂרָאֵ֗ל אֶל יְה֫וָה כִּֽי עִם יְהוָ֥ה הַחֶ֑סֶד וְהַרְבֵּ֖ה עִמּ֣וֹ פְדֽוּת׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[PSAL 130:8]",
      "word_count": 5,
      "hebrew": "וְ֭הוּא יִפְדֶּ֣ה אֶת יִשְׂרָאֵ֑ל מִ֝כֹּ֗ל עֲוֺנֹתָֽיו׃"
    }
  }
}