    },
    "89": {
      "verse_number": 89,
      "sort_number": 89,
      "text": "[PSAL 119:89]",
      "word_count": 5,
      "hebrew": "לְעוֹלָ֥ם יְהוָ֑ה דְּ֝בָרְךָ֗ נִצָּ֥ב בַּשָּׁמָֽיִם׃"
    },
    "90": {
      "verse_number": 90,
      "sort_number": 90,
      "text": "[PSAL 119:90]",
      "word_count": 5,
      "hebrew": "לְדֹ֣ר וָ֭דֹר אֱמֽוּנָתֶ֑ךָ כּוֹנַ֥נְתָּ אֶ֝֗רֶץ וַֽתַּעֲמֹֽד׃"
    },
    "91": {
      "verse_number": 91,
      "sort_number": 91,
      "text": "[PSAL 119:91]",
      "word_count": 5,
      "hebrew": "לְֽ֭מִשְׁפָּטֶיךָ עָמְד֣וּ הַיּ֑וֹם כִּ֖י הַכֹּ֣ל עֲבָדֶֽיךָ׃"
    },
    "92": {
      "verse_number": 92,
      "sort_number": 92,
      "text": "[PSAL 119:92]",
