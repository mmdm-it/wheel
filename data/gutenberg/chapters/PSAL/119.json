    },
    "94": {
      "verse_number": 94,
      "sort_number": 94,
      "text": "[PSAL 119:94]",
      "word_count": 5,
      "hebrew": "לְֽךָ אֲ֭נִי הוֹשִׁיעֵ֑נִי כִּ֖י פִקּוּדֶ֣יךָ דָרָֽשְׁתִּי׃"
    },
    "95": {
      "verse_number": 95,
      "sort_number": 95,
      "text": "[PSAL 119:95]",
      "word_count": 5,
      "hebrew": "לִ֤י קִוּ֣וּ רְשָׁעִ֣ים לְאַבְּדֵ֑נִי עֵ֝דֹתֶ֗יךָ אֶתְבּוֹנָֽן׃"
    },
    "96": {
      "verse_number": 96,
      "sort_number": 96,
      "text": "[PSAL 119:96]",
      "word_count": 5,
      "hebrew": "לְֽכָל תִּ֭כְלָה רָאִ֣יתִי קֵ֑ץ רְחָבָ֖ה מִצְוָתְךָ֣ מְאֹֽד׃"
    },
    "97": {
      "verse_number": 97,
      "sort_number": 97,
      "text": "[PSAL 119:97]",
      "word_count": 5,
      "hebrew": "מָֽה אָהַ֥בְתִּי תוֹרָתֶ֑ךָ כָּל הַ֝יּ֗וֹם הִ֣יא שִׂיחָתִֽי׃"
    },
    "98": {
      "verse_number": 98,
      "sort_number": 98,
      "text": "[PSAL 119:98]",
      "word_count": 5,
      "hebrew": "מֵ֭אֹ֣יְבַי תְּחַכְּמֵ֣נִי מִצְוֺתֶ֑ךָ כִּ֖י לְעוֹלָ֣ם הִיא לִֽי׃"
    },
    "99": {
      "verse_number": 99,
      "sort_number": 99,
      "text": "[PSAL 119:99]",
      "word_count": 5,
      "hebrew": "מִכָּל מְלַמְּדַ֥י הִשְׂכַּ֑לְתִּי כִּ֥י עֵ֝דְוֺתֶ֗יךָ שִׂ֣יחָה לִֽֿי׃"
    },
    "100": {
      "verse_number": 100,
      "sort_number": 100,
      "text": "[PSAL 119:100]",
      "word_count": 5,
      "hebrew": "מִזְּקֵנִ֥ים אֶתְבּוֹנָ֑ן כִּ֖י פִקּוּדֶ֣יךָ נָצָֽרְתִּי׃"
    },
    "101": {
      "verse_number": 101,
      "sort_number": 101,
      "text": "[PSAL 119:101]",
      "word_count": 5,
      "hebrew": "מִכָּל אֹ֣רַח רָ֭ע כָּלִ֣אתִי רַגְלָ֑י לְ֝מַ֗עַן אֶשְׁמֹ֥ר דְּבָרֶֽךָ׃"
    },
    "102": {
      "verse_number": 102,
      "sort_number": 102,
      "text": "[PSAL 119:102]",
      "word_count": 5,
      "hebrew": "מִמִּשְׁפָּטֶ֥יךָ לֹא סָ֑רְתִּי כִּֽי אַ֝תָּ֗ה הוֹרֵתָֽנִי׃"
    },
    "103": {
      "verse_number": 103,
      "sort_number": 103,
      "text": "[PSAL 119:103]",
