{
  "chapter_id": "PSAL_119",
  "chapter_key": "119",
  "chapter_number": 119,
  "sort_number": 119,
  "name": "119",
  "book_key": "PSAL",
  "testament": "Vetus_Testamentum",
  "section": "Libri_Sapientiae",
  "verse_count": 176,
  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[PSAL 119:1]",
      "word_count": 5,
      "hebrew": "אַשְׁרֵ֥י תְמִֽימֵי דָ֑רֶךְ הַֽ֝הֹלְכִ֗ים בְּתוֹרַ֥ת יְהוָֽה׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[PSAL 119:2]",
      "word_count": 5,
      "hebrew": "אַ֭שְׁרֵי נֹצְרֵ֥י עֵדֹתָ֗יו בְּכָל לֵ֥ב יִדְרְשֽׁוּהוּ׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[PSAL 119:3]",
      "word_count": 5,
      "hebrew": "אַ֭ף לֹֽא פָעֲל֣וּ עַוְלָ֑ה בִּדְרָכָ֥יו הָלָֽכוּ׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[PSAL 119:4]",
      "word_count": 5,
      "hebrew": "אַ֭תָּה צִוִּ֥יתָה פִקֻּדֶ֗יךָ לִשְׁמֹ֥ר מְאֹֽד׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[PSAL 119:5]",
      "word_count": 5,
      "hebrew": "אַ֭חֲלַי יִכֹּ֥נוּ דְרָכָ֗י לִשְׁמֹ֥ר חֻקֶּֽיךָ׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[PSAL 119:6]",
      "word_count": 5,
      "hebrew": "אָ֥ז לֹא אֵב֑וֹשׁ בְּ֝הַבִּיטִ֗י אֶל כָּל מִצְוֺתֶֽיךָ׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[PSAL 119:7]",
      "word_count": 5,
      "hebrew": "א֭וֹדְךָ בְּיֹ֣שֶׁר לֵבָ֑ב בְּ֝לָמְדִ֗י מִשְׁפְּטֵ֥י צִדְקֶֽךָ׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[PSAL 119:8]",
      "word_count": 5,
      "hebrew": "אֶת חֻקֶּ֥יךָ אֶשְׁמֹ֑ר אַֽל תַּעַזְבֵ֥נִי עַד מְאֹֽד׃"
    },
    "9": {
      "verse_number": 9,
      "sort_number": 9,
      "text": "[PSAL 119:9]",
      "word_count": 5,
      "hebrew": "בַּמֶּ֣ה יְזַכֶּה נַּ֭עַר אֶת אָרְח֑וֹ לִ֝שְׁמֹ֗ר כִּדְבָרֶֽךָ׃"
    },
    "10": {
      "verse_number": 10,
      "sort_number": 10,
      "text": "[PSAL 119:10]",
      "word_count": 5,
      "hebrew": "בְּכָל לִבִּ֥י דְרַשְׁתִּ֑יךָ אַל תַּ֝שְׁגֵּ֗נִי מִמִּצְוֺתֶֽיךָ׃"
    },
    "11": {
      "verse_number": 11,
      "sort_number": 11,
      "text": "[PSAL 119:11]",
      "word_count": 5,
      "hebrew": "בְּ֭לִבִּי צָפַ֣נְתִּי אִמְרָתֶ֑ךָ לְ֝מַ֗עַן לֹ֣א אֶֽחֱטָא לָֽךְ׃"
    },
    "12": {
      "verse_number": 12,
      "sort_number": 12,
      "text": "[PSAL 119:12]",
      "word_count": 5,
      "hebrew": "בָּר֖וּךְ אַתָּ֥ה יְהוָ֗ה לַמְּדֵ֥נִי חֻקֶּֽיךָ׃"
    },
    "13": {
      "verse_number": 13,
      "sort_number": 13,
      "text": "[PSAL 119:13]",
      "word_count": 5,
      "hebrew": "בִּשְׂפָתַ֥י סִפַּ֑רְתִּי כֹּ֝֗ל מִשְׁפְּטֵי פִֽיךָ׃"
    },
    "14": {
      "verse_number": 14,
      "sort_number": 14,
      "text": "[PSAL 119:14]",
      "word_count": 5,
      "hebrew": "בְּדֶ֖רֶךְ עֵדְוֺתֶ֥יךָ שַׂ֗שְׂתִּי כְּעַ֣ל כָּל הֽוֹן׃"
    },
    "15": {
      "verse_number": 15,
      "sort_number": 15,
      "text": "[PSAL 119:15]",
      "word_count": 5,
      "hebrew": "בְּפִקֻּדֶ֥יךָ אָשִׂ֑יחָה וְ֝אַבִּ֗יטָה אֹרְחֹתֶֽיךָ׃"
    },
    "16": {
      "verse_number": 16,
      "sort_number": 16,
      "text": "[PSAL 119:16]",
      "word_count": 5,
      "hebrew": "בְּחֻקֹּתֶ֥יךָ אֶֽשְׁתַּעֲשָׁ֑ע לֹ֭א אֶשְׁכַּ֣ח דְּבָרֶֽךָ׃"
    },
    "17": {
      "verse_number": 17,
      "sort_number": 17,
      "text": "[PSAL 119:17]",
      "word_count": 5,
      "hebrew": "גְּמֹ֖ל עַֽל עַבְדְּךָ֥ אֶֽחְיֶ֗ה וְאֶשְׁמְרָ֥ה דְבָרֶֽךָ׃"
    },
    "18": {
      "verse_number": 18,
      "sort_number": 18,
      "text": "[PSAL 119:18]",
      "word_count": 5,
      "hebrew": "גַּל עֵינַ֥י וְאַבִּ֑יטָה נִ֝פְלָא֗וֹת מִתּוֹרָתֶֽךָ׃"
    },
    "19": {
      "verse_number": 19,
      "sort_number": 19,
      "text": "[PSAL 119:19]",
      "word_count": 5,
      "hebrew": "גֵּ֣ר אָנֹכִ֣י בָאָ֑רֶץ אַל תַּסְתֵּ֥ר מִ֝מֶּ֗נִּי מִצְוֺתֶֽיךָ׃"
    },
    "20": {
      "verse_number": 20,
      "sort_number": 20,
      "text": "[PSAL 119:20]",
      "word_count": 5,
      "hebrew": "גָּרְסָ֣ה נַפְשִׁ֣י לְתַאֲבָ֑ה אֶֽל מִשְׁפָּטֶ֥יךָ בְכָל עֵֽת׃"
    },
    "21": {
      "verse_number": 21,
      "sort_number": 21,
      "text": "[PSAL 119:21]",
      "word_count": 5,
      "hebrew": "גָּ֭עַרְתָּ זֵדִ֣ים אֲרוּרִ֑ים הַ֝שֹּׁגִים מִמִּצְוֺתֶֽיךָ׃"
    },
    "22": {
      "verse_number": 22,
      "sort_number": 22,
      "text": "[PSAL 119:22]",
      "word_count": 5,
      "hebrew": "גַּ֣ל מֵֽ֭עָלַי חֶרְפָּ֣ה וָב֑וּז כִּ֖י עֵדֹתֶ֣יךָ נָצָֽרְתִּי׃"
    },
    "23": {
      "verse_number": 23,
      "sort_number": 23,
      "text": "[PSAL 119:23]",
      "word_count": 5,
      "hebrew": "גַּ֤ם יָֽשְׁב֣וּ שָׂ֭רִים בִּ֣י נִדְבָּ֑רוּ עַ֝בְדְּךָ֗ יָשִׂ֥יחַ בְּחֻקֶּֽיךָ׃"
    },
    "24": {
      "verse_number": 24,
      "sort_number": 24,
      "text": "[PSAL 119:24]",
      "word_count": 5,
      "hebrew": "גַּֽם עֵ֭דֹתֶיךָ שַׁעֲשֻׁעָ֗י אַנְשֵׁ֥י עֲצָתִֽי׃"
    },
    "25": {
      "verse_number": 25,
      "sort_number": 25,
      "text": "[PSAL 119:25]",
      "word_count": 5,
      "hebrew": "דָּֽבְקָ֣ה לֶעָפָ֣ר נַפְשִׁ֑י חַ֝יֵּ֗נִי כִּדְבָרֶֽךָ׃"
    },
    "26": {
      "verse_number": 26,
      "sort_number": 26,
      "text": "[PSAL 119:26]",
      "word_count": 5,
      "hebrew": "דְּרָכַ֣י סִ֭פַּרְתִּי וַֽתַּעֲנֵ֗נִי לַמְּדֵ֥נִי חֻקֶּֽיךָ׃"
    },
    "27": {
      "verse_number": 27,
      "sort_number": 27,
      "text": "[PSAL 119:27]",
      "word_count": 5,
      "hebrew": "דֶּֽרֶךְ פִּקּוּדֶ֥יךָ הֲבִינֵ֑נִי וְ֝אָשִׂ֗יחָה בְּנִפְלְאוֹתֶֽיךָ׃"
    },
    "28": {
      "verse_number": 28,
      "sort_number": 28,
      "text": "[PSAL 119:28]",
      "word_count": 5,
      "hebrew": "דָּלְפָ֣ה נַ֭פְשִׁי מִתּוּגָ֑ה קַ֝יְּמֵ֗נִי כִּדְבָרֶֽךָ׃"
    },
    "29": {
      "verse_number": 29,
      "sort_number": 29,
      "text": "[PSAL 119:29]",
      "word_count": 5,
      "hebrew": "דֶּֽרֶךְ שֶׁ֭קֶר הָסֵ֣ר מִמֶּ֑נִּי וְֽתוֹרָתְךָ֥ חָנֵּֽנִי׃"
    },
    "30": {
      "verse_number": 30,
      "sort_number": 30,
      "text": "[PSAL 119:30]",
      "word_count": 5,
      "hebrew": "דֶּֽרֶךְ אֱמוּנָ֥ה בָחָ֑רְתִּי מִשְׁפָּטֶ֥יךָ שִׁוִּֽיתִי׃"
    },
    "31": {
      "verse_number": 31,
      "sort_number": 31,
      "text": "[PSAL 119:31]",
      "word_count": 5,
      "hebrew": "דָּבַ֥קְתִּי בְעֵֽדְוֺתֶ֑יךָ יְ֝הוָ֗ה אַל תְּבִישֵֽׁנִי׃"
    },
    "32": {
      "verse_number": 32,
      "sort_number": 32,
      "text": "[PSAL 119:32]",
      "word_count": 5,
      "hebrew": "דֶּֽרֶךְ מִצְוֺתֶ֥יךָ אָר֑וּץ כִּ֖י תַרְחִ֣יב לִבִּֽי׃"
    },
    "33": {
      "verse_number": 33,
      "sort_number": 33,
      "text": "[PSAL 119:33]",
      "word_count": 5,
      "hebrew": "הוֹרֵ֣נִי יְ֭הוָה דֶּ֥רֶךְ חֻקֶּ֗יךָ וְאֶצְּרֶ֥נָּה עֵֽקֶב׃"
    },
    "34": {
      "verse_number": 34,
      "sort_number": 34,
      "text": "[PSAL 119:34]",
      "word_count": 5,
      "hebrew": "הֲ֭בִינֵנִי וְאֶצְּרָ֥ה תֽוֹרָתֶ֗ךָ וְאֶשְׁמְרֶ֥נָּה בְכָל לֵֽב׃"
    },
    "35": {
      "verse_number": 35,
      "sort_number": 35,
      "text": "[PSAL 119:35]",
      "word_count": 5,
      "hebrew": "הַ֭דְרִיכֵנִי בִּנְתִ֣יב מִצְוֺתֶ֑יךָ כִּי ב֥וֹ חָפָֽצְתִּי׃"
    },
    "36": {
      "verse_number": 36,
      "sort_number": 36,
      "text": "[PSAL 119:36]",
      "word_count": 5,
      "hebrew": "הַט לִ֭בִּי אֶל עֵדְוֺתֶ֗יךָ וְאַ֣ל אֶל בָּֽצַע׃"
    },
    "37": {
      "verse_number": 37,
      "sort_number": 37,
      "text": "[PSAL 119:37]",
      "word_count": 5,
      "hebrew": "הַעֲבֵ֣ר עֵ֭ינַי מֵרְא֣וֹת שָׁ֑וְא בִּדְרָכֶ֥ךָ חַיֵּֽנִי׃"
    },
    "38": {
      "verse_number": 38,
      "sort_number": 38,
      "text": "[PSAL 119:38]",
      "word_count": 5,
      "hebrew": "הָקֵ֣ם לְ֭עַבְדְּךָ אִמְרָתֶ֑ךָ אֲ֝שֶׁ֗ר לְיִרְאָתֶֽךָ׃"
    },
    "39": {
      "verse_number": 39,
      "sort_number": 39,
      "text": "[PSAL 119:39]",
      "word_count": 5,
      "hebrew": "הַעֲבֵ֣ר חֶ֭רְפָּתִי אֲשֶׁ֣ר יָגֹ֑רְתִּי כִּ֖י מִשְׁפָּטֶ֣יךָ טוֹבִֽים׃"
    },
    "40": {
      "verse_number": 40,
      "sort_number": 40,
      "text": "[PSAL 119:40]",
      "word_count": 5,
      "hebrew": "הִ֭נֵּה תָּאַ֣בְתִּי לְפִקֻּדֶ֑יךָ בְּצִדְקָתְךָ֥ חַיֵּֽנִי׃"
    },
    "41": {
      "verse_number": 41,
      "sort_number": 41,
      "text": "[PSAL 119:41]",
      "word_count": 5,
      "hebrew": "וִֽיבֹאֻ֣נִי חֲסָדֶ֣ךָ יְהוָ֑ה תְּ֝שֽׁוּעָתְךָ֗ כְּאִמְרָתֶֽךָ׃"
    },
    "42": {
      "verse_number": 42,
      "sort_number": 42,
      "text": "[PSAL 119:42]",
      "word_count": 5,
      "hebrew": "וְאֶֽעֱנֶ֣ה חֹרְפִ֣י דָבָ֑ר כִּֽי בָ֝טַחְתִּי בִּדְבָרֶֽךָ׃"
    },
    "43": {
      "verse_number": 43,
      "sort_number": 43,
      "text": "[PSAL 119:43]",
      "word_count": 5,
      "hebrew": "וְֽאַל תַּצֵּ֬ל מִפִּ֣י דְבַר אֱמֶ֣ת עַד מְאֹ֑ד כִּ֖י לְמִשְׁפָּטֶ֣ךָ יִחָֽלְתִּי׃"
    },
    "44": {
      "verse_number": 44,
      "sort_number": 44,
      "text": "[PSAL 119:44]",
      "word_count": 5,
      "hebrew": "וְאֶשְׁמְרָ֖ה תוֹרָתְךָ֥ תָמִ֗יד לְעוֹלָ֥ם וָעֶֽד׃"
    },
    "45": {
      "verse_number": 45,
      "sort_number": 45,
      "text": "[PSAL 119:45]",
      "word_count": 5,
      "hebrew": "וְאֶתְהַלְּכָ֥ה בָרְחָבָ֑ה כִּ֖י פִקֻּדֶ֣יךָ דָרָֽשְׁתִּי׃"
    },
    "46": {
      "verse_number": 46,
      "sort_number": 46,
      "text": "[PSAL 119:46]",
      "word_count": 5,
      "hebrew": "וַאֲדַבְּרָ֣ה בְ֭עֵדֹתֶיךָ נֶ֥גֶד מְלָכִ֗ים וְלֹ֣א אֵבֽוֹשׁ׃"
    },
    "47": {
      "verse_number": 47,
      "sort_number": 47,
      "text": "[PSAL 119:47]",
      "word_count": 5,
      "hebrew": "וְאֶשְׁתַּֽעֲשַׁ֥ע בְּמִצְוֺתֶ֗יךָ אֲשֶׁ֣ר אָהָֽבְתִּי׃"
    },
    "48": {
      "verse_number": 48,
      "sort_number": 48,
      "text": "[PSAL 119:48]",
      "word_count": 5,
      "hebrew": "וְאֶשָּֽׂא כַפַּ֗י אֶֽל מִ֭צְוֺתֶיךָ אֲשֶׁ֥ר אָהָ֗בְתִּי וְאָשִׂ֥יחָה בְחֻקֶּֽיךָ׃"
    },
    "49": {
      "verse_number": 49,
      "sort_number": 49,
      "text": "[PSAL 119:49]",
      "word_count": 5,
      "hebrew": "זְכֹר דָּבָ֥ר לְעַבְדֶּ֑ךָ עַ֝֗ל אֲשֶׁ֣ר יִֽחַלְתָּֽנִי׃"
    },
    "50": {
      "verse_number": 50,
      "sort_number": 50,
      "text": "[PSAL 119:50]",
      "word_count": 5,
      "hebrew": "זֹ֣את נֶחָמָתִ֣י בְעָנְיִ֑י כִּ֖י אִמְרָתְךָ֣ חִיָּֽתְנִי׃"
    },
    "51": {
      "verse_number": 51,
      "sort_number": 51,
      "text": "[PSAL 119:51]",
      "word_count": 5,
      "hebrew": "זֵ֭דִים הֱלִיצֻ֣נִי עַד מְאֹ֑ד מִ֝תּֽוֹרָתְךָ֗ לֹ֣א נָטִֽיתִי׃"
    },
    "52": {
      "verse_number": 52,
      "sort_number": 52,
      "text": "[PSAL 119:52]",
      "word_count": 5,
      "hebrew": "זָ֘כַ֤רְתִּי מִשְׁפָּטֶ֖יךָ מֵעוֹלָ֥ם יְהוָ֗ה וָֽאֶתְנֶחָֽם׃"
    },
    "53": {
      "verse_number": 53,
      "sort_number": 53,
      "text": "[PSAL 119:53]",
      "word_count": 5,
      "hebrew": "זַלְעָפָ֣ה אֲ֭חָזַתְנִי מֵרְשָׁעִ֑ים עֹ֝זְבֵ֗י תּוֹרָתֶֽךָ׃"
    },
    "54": {
      "verse_number": 54,
      "sort_number": 54,
      "text": "[PSAL 119:54]",
      "word_count": 5,
      "hebrew": "זְ֭מִרוֹת הָֽיוּ לִ֥י חֻקֶּ֗יךָ בְּבֵ֣ית מְגוּרָֽי׃"
    },
    "55": {
      "verse_number": 55,
      "sort_number": 55,
      "text": "[PSAL 119:55]",
      "word_count": 5,
      "hebrew": "זָ֘כַ֤רְתִּי בַלַּ֣יְלָה שִׁמְךָ֣ יְהוָ֑ה וָֽ֝אֶשְׁמְרָ֗ה תּוֹרָתֶֽךָ׃"
    },
    "56": {
      "verse_number": 56,
      "sort_number": 56,
      "text": "[PSAL 119:56]",
      "word_count": 5,
      "hebrew": "זֹ֥את הָֽיְתָה לִּ֑י כִּ֖י פִקֻּדֶ֣יךָ נָצָֽרְתִּי׃"
    },
    "57": {
      "verse_number": 57,
      "sort_number": 57,
      "text": "[PSAL 119:57]",
      "word_count": 5,
      "hebrew": "חֶלְקִ֖י יְהוָ֥ה אָמַ֗רְתִּי לִשְׁמֹ֥ר דְּבָרֶֽיךָ׃"
    },
    "58": {
      "verse_number": 58,
      "sort_number": 58,
      "text": "[PSAL 119:58]",
      "word_count": 5,
      "hebrew": "חִלִּ֣יתִי פָנֶ֣יךָ בְכָל לֵ֑ב חָ֝נֵּ֗נִי כְּאִמְרָתֶֽךָ׃"
    },
    "59": {
      "verse_number": 59,
      "sort_number": 59,
      "text": "[PSAL 119:59]",
      "word_count": 5,
      "hebrew": "חִשַּׁ֥בְתִּי דְרָכָ֑י וָאָשִׁ֥יבָה רַ֝גְלַ֗י אֶל עֵדֹתֶֽיךָ׃"
    },
    "60": {
      "verse_number": 60,
      "sort_number": 60,
      "text": "[PSAL 119:60]",
      "word_count": 5,
      "hebrew": "חַ֭שְׁתִּי וְלֹ֣א הִתְמַהְמָ֑הְתִּי לִ֝שְׁמֹ֗ר מִצְוֺתֶֽיךָ׃"
    },
    "61": {
      "verse_number": 61,
      "sort_number": 61,
      "text": "[PSAL 119:61]",
      "word_count": 5,
      "hebrew": "חֶבְלֵ֣י רְשָׁעִ֣ים עִוְּדֻ֑נִי תּֽ֝וֹרָתְךָ֗ לֹ֣א שָׁכָֽחְתִּי׃"
    },
    "62": {
      "verse_number": 62,
      "sort_number": 62,
      "text": "[PSAL 119:62]",
      "word_count": 5,
      "hebrew": "חֲצֽוֹת לַ֗יְלָה אָ֭קוּם לְהוֹד֣וֹת לָ֑ךְ עַ֝֗ל מִשְׁפְּטֵ֥י צִדְקֶֽךָ׃"
    },
    "63": {
      "verse_number": 63,
      "sort_number": 63,
      "text": "[PSAL 119:63]",
      "word_count": 5,
      "hebrew": "חָבֵ֣ר אָ֭נִי לְכָל אֲשֶׁ֣ר יְרֵא֑וּךָ וּ֝לְשֹׁמְרֵ֗י פִּקּוּדֶֽיךָ׃"
    },
    "64": {
      "verse_number": 64,
      "sort_number": 64,
      "text": "[PSAL 119:64]",
      "word_count": 5,
      "hebrew": "חַסְדְּךָ֣ יְ֭הוָה מָלְאָ֥ה הָאָ֗רֶץ חֻקֶּ֥יךָ לַמְּדֵֽנִי׃"
    },
    "65": {
      "verse_number": 65,
      "sort_number": 65,
      "text": "[PSAL 119:65]",
      "word_count": 5,
      "hebrew": "ט֭וֹב עָשִׂ֣יתָ עִֽם עַבְדְּךָ֑ יְ֝הוָ֗ה כִּדְבָרֶֽךָ׃"
    },
    "66": {
      "verse_number": 66,
      "sort_number": 66,
      "text": "[PSAL 119:66]",
      "word_count": 5,
      "hebrew": "ט֤וּב טַ֣עַם וָדַ֣עַת לַמְּדֵ֑נִי כִּ֖י בְמִצְוֺתֶ֣יךָ הֶאֱמָֽנְתִּי׃"
    },
    "67": {
      "verse_number": 67,
      "sort_number": 67,
      "text": "[PSAL 119:67]",
      "word_count": 5,
      "hebrew": "טֶ֣רֶם אֶ֭עֱנֶה אֲנִ֣י שֹׁגֵ֑ג וְ֝עַתָּ֗ה אִמְרָתְךָ֥ שָׁמָֽרְתִּי׃"
    },
    "68": {
      "verse_number": 68,
      "sort_number": 68,
      "text": "[PSAL 119:68]",
      "word_count": 5,
      "hebrew": "טוֹב אַתָּ֥ה וּמֵטִ֗יב לַמְּדֵ֥נִי חֻקֶּֽיךָ׃"
    },
    "69": {
      "verse_number": 69,
      "sort_number": 69,
      "text": "[PSAL 119:69]",
      "word_count": 5,
      "hebrew": "טָפְל֬וּ עָלַ֣י שֶׁ֣קֶר זֵדִ֑ים אֲ֝נִ֗י בְּכָל לֵ֤ב אֱצֹּ֬ר פִּקּוּדֶֽיךָ׃"
    },
    "70": {
      "verse_number": 70,
      "sort_number": 70,
      "text": "[PSAL 119:70]",
      "word_count": 5,
      "hebrew": "טָפַ֣שׁ כַּחֵ֣לֶב לִבָּ֑ם אֲ֝נִ֗י תּוֹרָתְךָ֥ שִֽׁעֲשָֽׁעְתִּי׃"
    },
    "71": {
      "verse_number": 71,
      "sort_number": 71,
      "text": "[PSAL 119:71]",
      "word_count": 5,
      "hebrew": "טֽוֹב לִ֥י כִֽי עֻנֵּ֑יתִי לְ֝מַ֗עַן אֶלְמַ֥ד חֻקֶּֽיךָ׃"
    },
    "72": {
      "verse_number": 72,
      "sort_number": 72,
      "text": "[PSAL 119:72]",
      "word_count": 5,
      "hebrew": "טֽוֹב לִ֥י תֽוֹרַת פִּ֑יךָ מֵ֝אַלְפֵ֗י זָהָ֥ב וָכָֽסֶף׃"
    },
    "73": {
      "verse_number": 73,
      "sort_number": 73,
      "text": "[PSAL 119:73]",
      "word_count": 5,
      "hebrew": "יָדֶ֣יךָ עָ֭שׂוּנִי וַֽיְכוֹנְנ֑וּנִי הֲ֝בִינֵ֗נִי וְאֶלְמְדָ֥ה מִצְוֺתֶֽיךָ׃"
    },
    "74": {
      "verse_number": 74,
      "sort_number": 74,
      "text": "[PSAL 119:74]",
      "word_count": 5,
      "hebrew": "יְ֭רֵאֶיךָ יִרְא֣וּנִי וְיִשְׂמָ֑חוּ כִּ֖י לִדְבָרְךָ֣ יִחָֽלְתִּי׃"
    },
    "75": {
      "verse_number": 75,
      "sort_number": 75,
      "text": "[PSAL 119:75]",
      "word_count": 5,
      "hebrew": "יָדַ֣עְתִּי יְ֭הוָה כִּי צֶ֣דֶק מִשְׁפָּטֶ֑יךָ וֶ֝אֱמוּנָ֗ה עִנִּיתָֽנִי׃"
    },
    "76": {
      "verse_number": 76,
      "sort_number": 76,
      "text": "[PSAL 119:76]",
      "word_count": 5,
      "hebrew": "יְהִי נָ֣א חַסְדְּךָ֣ לְנַחֲמֵ֑נִי כְּאִמְרָתְךָ֥ לְעַבְדֶּֽךָ׃"
    },
    "77": {
      "verse_number": 77,
      "sort_number": 77,
      "text": "[PSAL 119:77]",
      "word_count": 5,
      "hebrew": "יְבֹא֣וּנִי רַחֲמֶ֣יךָ וְאֶֽחְיֶ֑ה כִּי תֽ֝וֹרָתְךָ֗ שַֽׁעֲשֻׁעָֽי׃"
    },
    "78": {
      "verse_number": 78,
      "sort_number": 78,
      "text": "[PSAL 119:78]",
      "word_count": 5,
      "hebrew": "יֵבֹ֣שׁוּ זֵ֭דִים כִּי שֶׁ֣קֶר עִוְּת֑וּנִי אֲ֝נִ֗י אָשִׂ֥יחַ בְּפִקּוּדֶֽיךָ׃"
    },
    "79": {
      "verse_number": 79,
      "sort_number": 79,
      "text": "[PSAL 119:79]",
      "word_count": 5,
      "hebrew": "יָשׁ֣וּבוּ לִ֣י יְרֵאֶ֑יךָ וידעו וְ֝יֹדְעֵ֗י עֵדֹתֶֽיךָ׃"
    },
    "80": {
      "verse_number": 80,
      "sort_number": 80,
      "text": "[PSAL 119:80]",
      "word_count": 5,
      "hebrew": "יְהִֽי לִבִּ֣י תָמִ֣ים בְּחֻקֶּ֑יךָ לְ֝מַ֗עַן לֹ֣א אֵבֽוֹשׁ׃"
    },
    "81": {
      "verse_number": 81,
      "sort_number": 81,
      "text": "[PSAL 119:81]",
      "word_count": 5,
      "hebrew": "כָּלְתָ֣ה לִתְשׁוּעָתְךָ֣ נַפְשִׁ֑י לִדְבָרְךָ֥ יִחָֽלְתִּי׃"
    },
    "82": {
      "verse_number": 82,
      "sort_number": 82,
      "text": "[PSAL 119:82]",
      "word_count": 5,
      "hebrew": "כָּל֣וּ עֵ֭ינַי לְאִמְרָתֶ֑ךָ לֵ֝אמֹ֗ר מָתַ֥י תְּֽנַחֲמֵֽנִי׃"
