  "chapter_number": 25,
  "sort_number": 25,
  "name": "25",
  "book_key": "PSAL",
  "testament": "Vetus_Testamentum",
  "section": "Libri_Sapientiae",
  "verse_count": 22,
  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[PSAL 25:1]",
      "word_count": 5,
      "hebrew": "לְדָוִ֡ד אֵלֶ֥יךָ יְ֝הוָ֗ה נַפְשִׁ֥י אֶשָּֽׂא׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[PSAL 25:2]",
      "word_count": 5,
      "hebrew": "אֱֽלֹהַ֗י בְּךָ֣ בָ֭טַחְתִּי אַל אֵב֑וֹשָׁה אַל יַֽעַלְצ֖וּ אֹיְבַ֣י לִֽי׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[PSAL 25:3]",
      "word_count": 5,
      "hebrew": "גַּ֣ם כָּל קֹ֭וֶיךָ לֹ֣א יֵבֹ֑שׁוּ יֵ֝בֹ֗שׁוּ הַבּוֹגְדִ֥ים רֵיקָֽם׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[PSAL 25:4]",
      "word_count": 5,
      "hebrew": "דְּרָכֶ֣יךָ יְ֭הוָה הוֹדִיעֵ֑נִי אֹ֖רְחוֹתֶ֣יךָ לַמְּדֵֽנִי׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[PSAL 25:5]",
      "word_count": 5,
      "hebrew": "הַדְרִ֘יכֵ֤נִי בַאֲמִתֶּ֨ךָ וְֽלַמְּדֵ֗נִי כִּֽי אַ֭תָּה אֱלֹהֵ֣י יִשְׁעִ֑י אוֹתְךָ֥ קִ֝וִּ֗יתִי כָּל הַיּֽוֹם׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[PSAL 25:6]",
      "word_count": 5,
      "hebrew": "זְכֹר רַחֲמֶ֣יךָ יְ֭הוָה וַחֲסָדֶ֑יךָ כִּ֖י מֵעוֹלָ֣ם הֵֽמָּה׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[PSAL 25:7]",
      "word_count": 5,
      "hebrew": "חַטֹּ֤אות נְעוּרַ֨י וּפְשָׁעַ֗י אַל תִּ֫זְכֹּ֥ר כְּחַסְדְּךָ֥ זְכָר לִי אַ֑תָּה לְמַ֖עַן טוּבְךָ֣ יְהוָֽה׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[PSAL 25:8]",
      "word_count": 5,
      "hebrew": "טוֹב וְיָשָׁ֥ר יְהוָ֑ה עַל כֵּ֤ן יוֹרֶ֖ה חַטָּאִ֣ים בַּדָּֽרֶךְ׃"
    },
    "9": {
      "verse_number": 9,
      "sort_number": 9,
      "text": "[PSAL 25:9]",
      "word_count": 5,
      "hebrew": "יַדְרֵ֣ךְ עֲ֭נָוִים בַּמִּשְׁפָּ֑ט וִֽילַמֵּ֖ד עֲנָוִ֣ים דַּרְכּֽוֹ׃"
    },
    "10": {
      "verse_number": 10,
      "sort_number": 10,
      "text": "[PSAL 25:10]",
      "word_count": 5,
      "hebrew": "כָּל אָרְח֣וֹת יְ֭הוָה חֶ֣סֶד וֶאֱמֶ֑ת לְנֹצְרֵ֥י בְ֝רִית֗וֹ וְעֵדֹתָֽיו׃"
    },
    "11": {
      "verse_number": 11,
      "sort_number": 11,
      "text": "[PSAL 25:11]",
      "word_count": 5,
      "hebrew": "לְמַֽעַן שִׁמְךָ֥ יְהוָ֑ה וְֽסָלַחְתָּ֥ לַ֝עֲוֺנִ֗י כִּ֣י רַב הֽוּא׃"
    },
    "12": {
      "verse_number": 12,
      "sort_number": 12,
      "text": "[PSAL 25:12]",
      "word_count": 5,
      "hebrew": "מִי זֶ֣ה הָ֭אִישׁ יְרֵ֣א יְהוָ֑ה י֝וֹרֶ֗נּוּ בְּדֶ֣רֶךְ יִבְחָֽר׃"
    },
    "13": {
      "verse_number": 13,
      "sort_number": 13,
      "text": "[PSAL 25:13]",
      "word_count": 5,
      "hebrew": "נַ֭פְשׁוֹ בְּט֣וֹב תָּלִ֑ין וְ֝זַרְע֗וֹ יִ֣ירַשׁ אָֽרֶץ׃"
    },
    "14": {
      "verse_number": 14,
      "sort_number": 14,
      "text": "[PSAL 25:14]",
      "word_count": 5,
      "hebrew": "ס֣וֹד יְ֭הוָה לִירֵאָ֑יו וּ֝בְרִית֗וֹ לְהוֹדִיעָֽם׃"
    },
    "15": {
      "verse_number": 15,
      "sort_number": 15,
      "text": "[PSAL 25:15]",
      "word_count": 5,
      "hebrew": "עֵינַ֣י תָּ֭מִיד אֶל יְהוָ֑ה כִּ֤י הֽוּא יוֹצִ֖יא מֵרֶ֣שֶׁת רַגְלָֽי׃"
    },
    "16": {
      "verse_number": 16,
      "sort_number": 16,
      "text": "[PSAL 25:16]",
      "word_count": 5,
      "hebrew": "פְּנֵה אֵלַ֥י וְחָנֵּ֑נִי כִּֽי יָחִ֖יד וְעָנִ֣י אָֽנִי׃"
    },
    "17": {
      "verse_number": 17,
      "sort_number": 17,
      "text": "[PSAL 25:17]",
      "word_count": 5,
      "hebrew": "צָר֣וֹת לְבָבִ֣י הִרְחִ֑יבוּ מִ֝מְּצֽוּקוֹתַ֗י הוֹצִיאֵֽנִי׃"
    },
    "18": {
      "verse_number": 18,
      "sort_number": 18,
      "text": "[PSAL 25:18]",
      "word_count": 5,
      "hebrew": "רְאֵ֣ה עָ֭נְיִי וַעֲמָלִ֑י וְ֝שָׂ֗א לְכָל חַטֹּאותָֽי׃"
    },
    "19": {
      "verse_number": 19,
      "sort_number": 19,
      "text": "[PSAL 25:19]",
      "word_count": 5,
      "hebrew": "רְאֵֽה אוֹיְבַ֥י כִּי רָ֑בּוּ וְשִׂנְאַ֖ת חָמָ֣ס שְׂנֵאֽוּנִי׃"
    },
    "20": {
      "verse_number": 20,
      "sort_number": 20,
      "text": "[PSAL 25:20]",
      "word_count": 5,
      "hebrew": "שָׁמְרָ֣ה נַ֭פְשִׁי וְהַצִּילֵ֑נִי אַל אֵ֝ב֗וֹשׁ כִּֽי חָסִ֥יתִי בָֽךְ׃"
    },
    "21": {
      "verse_number": 21,
      "sort_number": 21,
      "text": "[PSAL 25:21]",
      "word_count": 5,
      "hebrew": "תֹּם וָיֹ֥שֶׁר יִצְּר֑וּנִי כִּ֝֗י קִוִּיתִֽיךָ׃"
    },
    "22": {
      "verse_number": 22,
      "sort_number": 22,
      "text": "[PSAL 25:22]",
      "word_count": 5,
      "hebrew": "פְּדֵ֣ה אֱ֭לֹהִים אֶת יִשְׂרָאֵ֑ל מִ֝כֹּ֗ל צָֽרוֹתָיו׃"
    }
  }
}