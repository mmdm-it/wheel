  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[PSAL 87:1]",
      "word_count": 5,
      "hebrew": "לִבְנֵי קֹ֭רַח מִזְמ֣וֹר שִׁ֑יר יְ֝סוּדָת֗וֹ בְּהַרְרֵי קֹֽדֶשׁ׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[PSAL 87:2]",
      "word_count": 5,
      "hebrew": "אֹהֵ֣ב יְ֭הוָה שַׁעֲרֵ֣י צִיּ֑וֹן מִ֝כֹּ֗ל מִשְׁכְּנ֥וֹת יַעֲקֹֽב׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[PSAL 87:3]",
      "word_count": 5,
      "hebrew": "נִ֭כְבָּדוֹת מְדֻבָּ֣ר בָּ֑ךְ עִ֖יר הָאֱלֹהִ֣ים סֶֽלָה׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[PSAL 87:4]",
      "word_count": 5,
      "hebrew": "אַזְכִּ֤יר רַ֥הַב וּבָבֶ֗ל לְֽיֹ֫דְעָ֥י הִנֵּ֤ה פְלֶ֣שֶׁת וְצ֣וֹר עִם כּ֑וּשׁ זֶ֝֗ה יֻלַּד שָֽׁם׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[PSAL 87:5]",
      "word_count": 5,
      "hebrew": "וּֽלֲצִיּ֨וֹן יֵאָמַ֗ר אִ֣ישׁ וְ֭אִישׁ יֻלַּד בָּ֑הּ וְה֖וּא יְכוֹנְנֶ֣הָ עֶלְיֽוֹן׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[PSAL 87:6]",
      "word_count": 5,
      "hebrew": "יְֽהוָ֗ה יִ֭סְפֹּר בִּכְת֣וֹב עַמִּ֑ים זֶ֖ה יֻלַּד שָׁ֣ם סֶֽלָה׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[PSAL 87:7]",
      "word_count": 5,
      "hebrew": "וְשָׁרִ֥ים כְּחֹלְלִ֑ים כָּֽל מַעְיָנַ֥י בָּֽךְ׃"
    }
  }
}