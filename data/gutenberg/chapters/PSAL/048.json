  "name": "48",
  "book_key": "PSAL",
  "testament": "Vetus_Testamentum",
  "section": "Libri_Sapientiae",
  "verse_count": 14,
  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[PSAL 48:1]",
      "word_count": 5,
      "hebrew": "שִׁ֥יר מִ֝זְמוֹר לִבְנֵי קֹֽרַח׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[PSAL 48:2]",
      "word_count": 5,
      "hebrew": "גָּ֘ד֤וֹל יְהוָ֣ה וּמְהֻלָּ֣ל מְאֹ֑ד בְּעִ֥יר אֱ֝לֹהֵ֗ינוּ הַר קָדְשֽׁוֹ׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[PSAL 48:3]",
      "word_count": 5,
      "hebrew": "יְפֵ֥ה נוֹף֮ מְשׂ֪וֹשׂ כָּל הָ֫אָ֥רֶץ הַר צִ֭יּוֹן יַרְכְּתֵ֣י צָפ֑וֹן קִ֝רְיַ֗ת מֶ֣לֶךְ רָֽב׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[PSAL 48:4]",
      "word_count": 5,
      "hebrew": "אֱלֹהִ֥ים בְּאַרְמְנוֹתֶ֗יהָ נוֹדַ֥ע לְמִשְׂגָּֽב׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[PSAL 48:5]",
      "word_count": 5,
      "hebrew": "כִּֽי הִנֵּ֣ה הַ֭מְּלָכִים נֽוֹעֲד֑וּ עָבְר֥וּ יַחְדָּֽו׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[PSAL 48:6]",
      "word_count": 5,
      "hebrew": "הֵ֣מָּה רָ֭אוּ כֵּ֣ן תָּמָ֑הוּ נִבְהֲל֥וּ נֶחְפָּֽזוּ׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[PSAL 48:7]",
      "word_count": 5,
      "hebrew": "רְ֭עָדָה אֲחָזָ֣תַם שָׁ֑ם חִ֝֗יל כַּיּוֹלֵֽדָה׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[PSAL 48:8]",
      "word_count": 5,
      "hebrew": "בְּר֥וּחַ קָדִ֑ים תְּ֝שַׁבֵּ֗ר אֳנִיּ֥וֹת תַּרְשִֽׁישׁ׃"
    },
    "9": {
      "verse_number": 9,
      "sort_number": 9,
      "text": "[PSAL 48:9]",
      "word_count": 5,
      "hebrew": "כַּאֲשֶׁ֤ר שָׁמַ֨עְנוּ כֵּ֤ן רָאִ֗ינוּ בְּעִיר יְהוָ֣ה צְ֭בָאוֹת בְּעִ֣יר אֱלֹהֵ֑ינוּ אֱלֹ֘הִ֤ים יְכוֹנְנֶ֖הָ עַד עוֹלָ֣ם סֶֽלָה׃"
    },
    "10": {
      "verse_number": 10,
      "sort_number": 10,
      "text": "[PSAL 48:10]",
      "word_count": 5,
      "hebrew": "דִּמִּ֣ינוּ אֱלֹהִ֣ים חַסְדֶּ֑ךָ בְּ֝קֶ֗רֶב הֵיכָלֶֽךָ׃"
    },
    "11": {
      "verse_number": 11,
      "sort_number": 11,
      "text": "[PSAL 48:11]",
      "word_count": 5,
      "hebrew": "כְּשִׁמְךָ֤ אֱלֹהִ֗ים כֵּ֣ן תְּ֭הִלָּתְךָ עַל קַצְוֵי אֶ֑רֶץ צֶ֝֗דֶק מָלְאָ֥ה יְמִינֶֽךָ׃"
    },
    "12": {
      "verse_number": 12,
      "sort_number": 12,
      "text": "[PSAL 48:12]",
      "word_count": 5,
      "hebrew": "יִשְׂמַ֤ח הַר צִיּ֗וֹן תָּ֭גֵלְנָה בְּנ֣וֹת יְהוּדָ֑ה לְ֝מַ֗עַן מִשְׁפָּטֶֽיךָ׃"
    },
    "13": {
      "verse_number": 13,
      "sort_number": 13,
      "text": "[PSAL 48:13]",
      "word_count": 5,
      "hebrew": "סֹ֣בּוּ צִ֭יּוֹן וְהַקִּיפ֑וּהָ סִ֝פְר֗וּ מִגְדָּלֶֽיהָ׃"
    },
    "14": {
      "verse_number": 14,
      "sort_number": 14,
      "text": "[PSAL 48:14]",
      "word_count": 5,
      "hebrew": "שִׁ֤יתוּ לִבְּכֶ֨ם לְֽחֵילָ֗ה פַּסְּג֥וּ אַרְמְנוֹתֶ֑יהָ לְמַ֥עַן תְּ֝סַפְּר֗וּ לְד֣וֹר אַחֲרֽוֹן׃"
    }
  }
}