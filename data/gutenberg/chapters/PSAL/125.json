  "verses": {
    "1": {
      "seq": 1,
      "v_in": {
        "MT": 1,
        "VUL": 1,
        "LXX": 1
      },
      "text": {
        "WLC": "שִׁ֗יר הַֽמַּ֫עֲל֥וֹת הַבֹּטְחִ֥ים בַּיהוָ֑ה כְּֽהַר צִיּ֥וֹן לֹא יִ֝מּ֗וֹט לְעוֹלָ֥ם יֵשֵֽׁב׃"
      }
    },
    "2": {
      "seq": 2,
      "v_in": {
        "MT": 2,
        "VUL": 2,
        "LXX": 2
      },
      "text": {
        "WLC": "יְֽרוּשָׁלִַ֗ם הָרִים֮ סָבִ֪יב לָ֥הּ וַ֭יהוָה סָבִ֣יב לְעַמּ֑וֹ מֵ֝עַתָּ֗ה וְעַד עוֹלָֽם׃"
      }
    },
    "3": {
      "seq": 3,
      "v_in": {
        "MT": 3,
        "VUL": 3,
        "LXX": 3
      },
      "text": {
        "WLC": "כִּ֤י לֹ֪א יָנ֡וּחַ שֵׁ֤בֶט הָרֶ֗שַׁע עַל֮ גּוֹרַ֪ל הַֽצַּדִּ֫יקִ֥ים לְמַ֡עַן לֹא יִשְׁלְח֖וּ הַצַּדִּיקִ֨ים בְּעַוְלָ֬תָה יְדֵיהֶֽם׃"
      }
    },
    "4": {
      "seq": 4,
      "v_in": {
        "MT": 4,
        "VUL": 4,
        "LXX": 4
      },
      "text": {
        "WLC": "הֵיטִ֣יבָה יְ֭הוָה לַטּוֹבִ֑ים וְ֝לִֽישָׁרִ֗ים בְּלִבּוֹתָֽם׃"
      }
    },
    "5": {
      "seq": 5,
      "v_in": {
        "MT": 5,
        "VUL": 5,
        "LXX": 5
      },
      "text": {
        "WLC": "וְהַמַּטִּ֤ים עַֽקַלְקַלּוֹתָ֗ם יוֹלִיכֵ֣ם יְ֭הוָה אֶת פֹּעֲלֵ֣י הָאָ֑וֶן שָׁ֝ל֗וֹם עַל יִשְׂרָאֵֽל׃"
      }
    }
  }
}