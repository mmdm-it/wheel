  "testament": "Vetus_Testamentum",
  "section": "Libri_Sapientiae",
  "verse_count": 14,
  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[IOB 26:1]",
      "word_count": 5,
      "hebrew": "וַיַּ֥עַן אִיּ֗וֹב וַיֹּאמַֽר׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[IOB 26:2]",
      "word_count": 5,
      "hebrew": "מֶה עָזַ֥רְתָּ לְלֹא כֹ֑חַ ה֝וֹשַׁ֗עְתָּ זְר֣וֹעַ לֹא עֹֽז׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[IOB 26:3]",
      "word_count": 5,
      "hebrew": "מַה יָּ֭עַצְתָּ לְלֹ֣א חָכְמָ֑ה וְ֝תוּשִׁיָּ֗ה לָרֹ֥ב הוֹדָֽעְתָּ׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[IOB 26:4]",
      "word_count": 5,
      "hebrew": "אֶת מִ֭י הִגַּ֣דְתָּ מִלִּ֑ין וְנִשְׁמַת מִ֝י יָצְאָ֥ה מִמֶּֽךָּ׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[IOB 26:5]",
      "word_count": 5,
      "hebrew": "הָרְפָאִ֥ים יְחוֹלָ֑לוּ מִתַּ֥חַת מַ֝֗יִם וְשֹׁכְנֵיהֶֽם׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[IOB 26:6]",
      "word_count": 5,
      "hebrew": "עָר֣וֹם שְׁא֣וֹל נֶגְדּ֑וֹ וְאֵ֥ין כְּ֝ס֗וּת לָֽאֲבַדּֽוֹן׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[IOB 26:7]",
      "word_count": 5,
      "hebrew": "נֹטֶ֣ה צָפ֣וֹן עַל תֹּ֑הוּ תֹּ֥לֶה אֶ֝֗רֶץ עַל בְּלִי מָֽה׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[IOB 26:8]",
      "word_count": 5,
      "hebrew": "צֹרֵֽר מַ֥יִם בְּעָבָ֑יו וְלֹא נִבְקַ֖ע עָנָ֣ן תַּחְתָּֽם׃"
    },
    "9": {
      "verse_number": 9,
      "sort_number": 9,
      "text": "[IOB 26:9]",
      "word_count": 5,
      "hebrew": "מְאַחֵ֥ז פְּנֵי כִסֵּ֑ה פַּרְשֵׁ֖ז עָלָ֣יו עֲנָנֽוֹ׃"
    },
    "10": {
      "verse_number": 10,
      "sort_number": 10,
      "text": "[IOB 26:10]",
      "word_count": 5,
      "hebrew": "חֹֽק חָ֭ג עַל פְּנֵי מָ֑יִם עַד תַּכְלִ֖ית א֣וֹר עִם חֹֽשֶׁךְ׃"
    },
    "11": {
      "verse_number": 11,
      "sort_number": 11,
      "text": "[IOB 26:11]",
      "word_count": 5,
      "hebrew": "עַמּוּדֵ֣י שָׁמַ֣יִם יְרוֹפָ֑פוּ וְ֝יִתְמְה֗וּ מִגַּעֲרָתֽוֹ׃"
    },
    "12": {
      "verse_number": 12,
      "sort_number": 12,
      "text": "[IOB 26:12]",
      "word_count": 5,
      "hebrew": "בְּ֭כֹחוֹ רָגַ֣ע הַיָּ֑ם ובתובנתו וּ֝בִתְבוּנָת֗וֹ מָ֣חַץ רָֽהַב׃"
    },
    "13": {
      "verse_number": 13,
      "sort_number": 13,
      "text": "[IOB 26:13]",
      "word_count": 5,
      "hebrew": "בְּ֭רוּחוֹ שָׁמַ֣יִם שִׁפְרָ֑ה חֹֽלֲלָ֥ה יָ֝ד֗וֹ נָחָ֥שׁ בָּרִֽיחַ׃"
    },
    "14": {
      "verse_number": 14,
      "sort_number": 14,
      "text": "[IOB 26:14]",
      "word_count": 5,
      "hebrew": "הֶן אֵ֤לֶּה קְצ֬וֹת דרכו דְּרָכָ֗יו וּמַה שֵּׁ֣מֶץ דָּ֭בָר נִשְׁמַע בּ֑וֹ וְרַ֥עַם גבורתו גְּ֝בוּרוֹתָ֗יו מִ֣י יִתְבּוֹנָֽן׃"
    }
  }
}