  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[PSAL 113:1]",
      "word_count": 5,
      "hebrew": "הַ֥לְלוּ יָ֨הּ הַ֭לְלוּ עַבְדֵ֣י יְהוָ֑ה הַֽ֝לְלוּ אֶת שֵׁ֥ם יְהוָֽה׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[PSAL 113:2]",
      "word_count": 5,
      "hebrew": "יְהִ֤י שֵׁ֣ם יְהוָ֣ה מְבֹרָ֑ךְ מֵֽ֝עַתָּ֗ה וְעַד עוֹלָֽם׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[PSAL 113:3]",
      "word_count": 5,
      "hebrew": "מִמִּזְרַח שֶׁ֥מֶשׁ עַד מְבוֹא֑וֹ מְ֝הֻלָּ֗ל שֵׁ֣ם יְהוָֽה׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[PSAL 113:4]",
      "word_count": 5,
      "hebrew": "רָ֖ם עַל כָּל גּוֹיִ֥ם יְהוָ֑ה עַ֖ל הַשָּׁמַ֣יִם כְּבוֹדֽוֹ׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[PSAL 113:5]",
      "word_count": 5,
      "hebrew": "מִ֭י כַּיהוָ֣ה אֱלֹהֵ֑ינוּ הַֽמַּגְבִּיהִ֥י לָשָֽׁבֶת׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[PSAL 113:6]",
      "word_count": 5,
      "hebrew": "הַֽמַּשְׁפִּילִ֥י לִרְא֑וֹת בַּשָּׁמַ֥יִם וּבָאָֽרֶץ׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[PSAL 113:7]",
      "word_count": 5,
      "hebrew": "מְקִֽימִ֣י מֵעָפָ֣ר דָּ֑ל מֵֽ֝אַשְׁפֹּ֗ת יָרִ֥ים אֶבְיֽוֹן׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[PSAL 113:8]",
      "word_count": 5,
      "hebrew": "לְהוֹשִׁיבִ֥י עִם נְדִיבִ֑ים עִ֝֗ם נְדִיבֵ֥י עַמּֽוֹ׃"
    },
    "9": {
      "verse_number": 9,
      "sort_number": 9,
      "text": "[PSAL 113:9]",
      "word_count": 5,
      "hebrew": "מֽוֹשִׁיבִ֨י עֲקֶ֬רֶת הַבַּ֗יִת אֵֽם הַבָּנִ֥ים שְׂמֵחָ֗ה הַֽלְלוּ יָֽהּ׃"
    }
  }
}