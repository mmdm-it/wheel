  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[PSAL 129:1]",
      "word_count": 5,
      "hebrew": "שִׁ֗יר הַֽמַּ֫עֲל֥וֹת רַ֭בַּת צְרָר֣וּנִי מִנְּעוּרַ֑י יֹֽאמַר נָ֝א יִשְׂרָאֵֽל׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[PSAL 129:2]",
      "word_count": 5,
      "hebrew": "רַ֭בַּת צְרָר֣וּנִי מִנְּעוּרָ֑י גַּ֝ם לֹא יָ֥כְלוּ לִֽי׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[PSAL 129:3]",
      "word_count": 5,
      "hebrew": "עַל גַּ֭בִּי חָרְשׁ֣וּ חֹרְשִׁ֑ים הֶ֝אֱרִ֗יכוּ למענותם לְמַעֲנִיתָֽם׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[PSAL 129:4]",
      "word_count": 5,
      "hebrew": "יְהוָ֥ה צַדִּ֑יק קִ֝צֵּ֗ץ עֲב֣וֹת רְשָׁעִֽים׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[PSAL 129:5]",
      "word_count": 5,
      "hebrew": "יֵ֭בֹשׁוּ וְיִסֹּ֣גוּ אָח֑וֹר כֹּ֝֗ל שֹׂנְאֵ֥י צִיּֽוֹן׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[PSAL 129:6]",
      "word_count": 5,
      "hebrew": "יִ֭הְיוּ כַּחֲצִ֣יר גַּגּ֑וֹת שֶׁקַּדְמַ֖ת שָׁלַ֣ף יָבֵֽשׁ׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[PSAL 129:7]",
      "word_count": 5,
      "hebrew": "שֶׁלֹּ֤א מִלֵּ֖א כַפּ֥וֹ קוֹצֵ֗ר וְחִצְנ֥וֹ מְעַמֵּֽר׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[PSAL 129:8]",
      "word_count": 5,
      "hebrew": "וְלֹ֤א אָֽמְר֨וּ הָעֹבְרִ֗ים בִּרְכַּֽת יְהוָ֥ה אֲלֵיכֶ֑ם בֵּרַ֥כְנוּ אֶ֝תְכֶ֗ם בְּשֵׁ֣ם יְהוָֽה׃"
    }
  }
}