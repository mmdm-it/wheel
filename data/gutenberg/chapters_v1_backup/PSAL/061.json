  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[PSAL 61:1]",
      "word_count": 5,
      "hebrew": "לַמְנַצֵּ֬חַ עַֽל נְגִינַ֬ת לְדָוִֽד׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[PSAL 61:2]",
      "word_count": 5,
      "hebrew": "שִׁמְעָ֣ה אֱ֭לֹהִים רִנָּתִ֑י הַ֝קְשִׁ֗יבָה תְּפִלָּתִֽי׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[PSAL 61:3]",
      "word_count": 5,
      "hebrew": "מִקְצֵ֤ה הָאָ֨רֶץ אֵלֶ֣יךָ אֶ֭קְרָא בַּעֲטֹ֣ף לִבִּ֑י בְּצוּר יָר֖וּם מִמֶּ֣נִּי תַנְחֵֽנִי׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[PSAL 61:4]",
      "word_count": 5,
      "hebrew": "כִּֽי הָיִ֣יתָ מַחְסֶ֣ה לִ֑י מִגְדַּל עֹ֝֗ז מִפְּנֵ֥י אוֹיֵֽב׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[PSAL 61:5]",
      "word_count": 5,
      "hebrew": "אָג֣וּרָה בְ֭אָהָלְךָ עוֹלָמִ֑ים אֶֽחֱסֶ֨ה בְסֵ֖תֶר כְּנָפֶ֣יךָ סֶּֽלָה׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[PSAL 61:6]",
      "word_count": 5,
      "hebrew": "כִּֽי אַתָּ֣ה אֱ֭לֹהִים שָׁמַ֣עְתָּ לִנְדָרָ֑י נָתַ֥תָּ יְ֝רֻשַּׁ֗ת יִרְאֵ֥י שְׁמֶֽךָ׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[PSAL 61:7]",
      "word_count": 5,
      "hebrew": "יָמִ֣ים עַל יְמֵי מֶ֣לֶךְ תּוֹסִ֑יף שְׁ֝נוֹתָ֗יו כְּמוֹ דֹ֥ר וָדֹֽר׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[PSAL 61:8]",
      "word_count": 5,
      "hebrew": "יֵשֵׁ֣ב ע֭וֹלָם לִפְנֵ֣י אֱלֹהִ֑ים חֶ֥סֶד וֶ֝אֱמֶ֗ת מַ֣ן יִנְצְרֻֽהוּ׃"
    }
  }
}