  "testament": "Vetus_Testamentum",
  "section": "Libri_Sapientiae",
  "verse_count": 12,
  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[PSAL 76:1]",
      "word_count": 5,
      "hebrew": "לַמְנַצֵּ֥חַ בִּנְגִינֹ֑ת מִזְמ֖וֹר לְאָסָ֣ף שִֽׁיר׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[PSAL 76:2]",
      "word_count": 5,
      "hebrew": "נוֹדָ֣ע בִּֽיהוּדָ֣ה אֱלֹהִ֑ים בְּ֝יִשְׂרָאֵ֗ל גָּד֥וֹל שְׁמֽוֹ׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[PSAL 76:3]",
      "word_count": 5,
      "hebrew": "וַיְהִ֣י בְשָׁלֵ֣ם סֻכּ֑וֹ וּמְע֖וֹנָת֣וֹ בְצִיּֽוֹן׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[PSAL 76:4]",
      "word_count": 5,
      "hebrew": "שָׁ֭מָּה שִׁבַּ֣ר רִשְׁפֵי קָ֑שֶׁת מָגֵ֬ן וְחֶ֖רֶב וּמִלְחָמָ֣ה סֶֽלָה׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[PSAL 76:5]",
      "word_count": 5,
      "hebrew": "נָ֭אוֹר אַתָּ֥ה אַדִּ֗יר מֵֽהַרְרֵי טָֽרֶף׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[PSAL 76:6]",
      "word_count": 5,
      "hebrew": "אֶשְׁתּוֹלְל֨וּ אַבִּ֣ירֵי לֵ֭ב נָמ֣וּ שְׁנָתָ֑ם וְלֹא מָצְא֖וּ כָל אַנְשֵׁי חַ֣יִל יְדֵיהֶֽם׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[PSAL 76:7]",
      "word_count": 5,
      "hebrew": "מִ֭גַּעֲרָ֣תְךָ אֱלֹהֵ֣י יַעֲקֹ֑ב נִ֝רְדָּ֗ם וְרֶ֣כֶב וָסֽוּס׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[PSAL 76:8]",
      "word_count": 5,
      "hebrew": "אַתָּ֤ה נ֥וֹרָא אַ֗תָּה וּמִֽי יַעֲמֹ֥ד לְפָנֶ֗יךָ מֵאָ֥ז אַפֶּֽךָ׃"
    },
    "9": {
      "verse_number": 9,
      "sort_number": 9,
      "text": "[PSAL 76:9]",
      "word_count": 5,
      "hebrew": "מִ֭שָּׁמַיִם הִשְׁמַ֣עְתָּ דִּ֑ין אֶ֖רֶץ יָֽרְאָ֣ה וְשָׁקָֽטָה׃"
    },
    "10": {
      "verse_number": 10,
      "sort_number": 10,
      "text": "[PSAL 76:10]",
      "word_count": 5,
      "hebrew": "בְּקוּם לַמִּשְׁפָּ֥ט אֱלֹהִ֑ים לְהוֹשִׁ֖יעַ כָּל עַנְוֵי אֶ֣רֶץ סֶֽלָה׃"
    },
    "11": {
      "verse_number": 11,
      "sort_number": 11,
      "text": "[PSAL 76:11]",
      "word_count": 5,
      "hebrew": "כִּֽי חֲמַ֣ת אָדָ֣ם תּוֹדֶ֑ךָּ שְׁאֵרִ֖ית חֵמֹ֣ת תַּחְגֹּֽר׃"
    },
    "12": {
      "verse_number": 12,
      "sort_number": 12,
      "text": "[PSAL 76:12]",
      "word_count": 5,
      "hebrew": "נִֽדֲר֣וּ וְשַׁלְּמוּ֮ לַיהוָ֪ה אֱֽלֹהֵ֫יכֶ֥ם כָּל סְבִיבָ֑יו יוֹבִ֥ילוּ שַׁ֝֗י לַמּוֹרָֽא׃"
    }
  }
}