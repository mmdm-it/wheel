  "book_key": "III_REG",
  "testament": "Vetus_Testamentum",
  "section": "Libri_Historici_et_Novellae_Biblicae",
  "verse_count": 34,
  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[III_REG 4:1]",
      "word_count": 5,
      "hebrew": "וַֽיְהִי֙ הַמֶּ֣לֶךְ שְׁלֹמֹ֔ה מֶ֖לֶךְ עַל כָּל יִשְׂרָאֵֽל׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[III_REG 4:2]",
      "word_count": 5,
      "hebrew": "וְאֵ֥לֶּה הַשָּׂרִ֖ים אֲשֶׁר ל֑וֹ עֲזַרְיָ֥הוּ בֶן צָד֖וֹק הַכֹּהֵֽן׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[III_REG 4:3]",
      "word_count": 5,
      "hebrew": "אֱלִיחֹ֧רֶף וַאֲחִיָּ֛ה בְּנֵ֥י שִׁישָׁ֖א סֹפְרִ֑ים יְהוֹשָׁפָ֥ט בֶּן אֲחִיל֖וּד הַמַּזְכִּֽיר׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[III_REG 4:4]",
      "word_count": 5,
      "hebrew": "וּבְנָיָ֥הוּ בֶן יְהוֹיָדָ֖ע עַל הַצָּבָ֑א וְצָד֥וֹק וְאֶבְיָתָ֖ר כֹּהֲנִֽים׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[III_REG 4:5]",
      "word_count": 5,
      "hebrew": "וַעֲזַרְיָ֥הוּ בֶן נָתָ֖ן עַל הַנִּצָּבִ֑ים וְזָב֧וּד בֶּן נָתָ֛ן כֹּהֵ֖ן רֵעֶ֥ה הַמֶּֽלֶךְ׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[III_REG 4:6]",
      "word_count": 5,
      "hebrew": "וַאֲחִישָׁ֖ר עַל הַבָּ֑יִת וַאֲדֹנִירָ֥ם בֶּן עַבְדָּ֖א עַל הַמַּֽס׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[III_REG 4:7]",
      "word_count": 5,
      "hebrew": "וְלִשְׁלֹמֹ֞ה שְׁנֵים עָשָׂ֤ר נִצָּבִים֙ עַל כָּל יִשְׂרָאֵ֔ל וְכִלְכְּל֥וּ אֶת הַמֶּ֖לֶךְ וְאֶת בֵּית֑וֹ חֹ֧דֶשׁ בַּשָּׁנָ֛ה יִהְיֶ֥ה עַל אחד הָאֶחָ֖ד לְכַלְכֵּֽל׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[III_REG 4:8]",
      "word_count": 5,
      "hebrew": "וְאֵ֣לֶּה שְׁמוֹתָ֔ם בֶּן ח֖וּר בְּהַ֥ר אֶפְרָֽיִם׃"
    },
    "9": {
      "verse_number": 9,
      "sort_number": 9,
      "text": "[III_REG 4:9]",
      "word_count": 5,
      "hebrew": "בֶּן דֶּ֛קֶר בְּמָקַ֥ץ וּבְשַֽׁעַלְבִ֖ים וּבֵ֣ית שָׁ֑מֶשׁ וְאֵיל֖וֹן בֵּ֥ית חָנָֽן׃"
    },
    "10": {
      "verse_number": 10,
      "sort_number": 10,
      "text": "[III_REG 4:10]",
      "word_count": 5,
      "hebrew": "בֶּן חֶ֖סֶד בָּֽאֲרֻבּ֑וֹת ל֥וֹ שֹׂכֹ֖ה וְכָל אֶ֥רֶץ חֵֽפֶר׃"
    },
    "11": {
      "verse_number": 11,
      "sort_number": 11,
      "text": "[III_REG 4:11]",
      "word_count": 5,
      "hebrew": "בֶּן אֲבִֽינָדָ֖ב כָּל נָ֣פַת דֹּ֑אר טָפַת֙ בַּת שְׁלֹמֹ֔ה הָ֥יְתָה לּ֖וֹ לְאִשָּֽׁה׃"
    },
    "12": {
      "verse_number": 12,
      "sort_number": 12,
      "text": "[III_REG 4:12]",
      "word_count": 5,
      "hebrew": "בַּֽעֲנָא֙ בֶּן אֲחִיל֔וּד תַּעְנַ֖ךְ וּמְגִדּ֑וֹ וְכָל בֵּ֣ית שְׁאָ֡ן אֲשֶׁר֩ אֵ֨צֶל צָרְתַ֜נָה מִתַּ֣חַת לְיִזְרְעֶ֗אל מִבֵּ֤ית שְׁאָן֙ עַ֚ד אָבֵ֣ל מְחוֹלָ֔ה עַ֖ד מֵעֵ֥בֶר לְיָקְמֳעָֽם׃"
    },
    "13": {
      "verse_number": 13,
      "sort_number": 13,
      "text": "[III_REG 4:13]",
      "word_count": 5,
      "hebrew": "בֶּן גֶּ֖בֶר בְּרָמֹ֣ת גִּלְעָ֑ד ל֡וֹ חַוֺּת֩ יָאִ֨יר בֶּן מְנַשֶּׁ֜ה אֲשֶׁ֣ר בַּגִּלְעָ֗ד ל֚וֹ חֶ֤בֶל אַרְגֹּב֙ אֲשֶׁ֣ר בַּבָּשָׁ֔ן שִׁשִּׁים֙ עָרִ֣ים גְּדֹל֔וֹת חוֹמָ֖ה וּבְרִ֥יחַ נְחֹֽשֶׁת׃"
    },
    "14": {
      "verse_number": 14,
      "sort_number": 14,
      "text": "[III_REG 4:14]",
      "word_count": 5,
      "hebrew": "אֲחִֽינָדָ֥ב בֶּן עִדֹּ֖א מַחֲנָֽיְמָה׃"
    },
    "15": {
      "verse_number": 15,
      "sort_number": 15,
      "text": "[III_REG 4:15]",
      "word_count": 5,
      "hebrew": "אֲחִימַ֖עַץ בְּנַפְתָּלִ֑י גַּם ה֗וּא לָקַ֛ח אֶת בָּשְׂמַ֥ת בַּת שְׁלֹמֹ֖ה לְאִשָּֽׁה׃"
    },
    "16": {
      "verse_number": 16,
      "sort_number": 16,
      "text": "[III_REG 4:16]",
      "word_count": 5,
      "hebrew": "בַּֽעֲנָא֙ בֶּן חוּשָׁ֔י בְּאָשֵׁ֖ר וּבְעָלֽוֹת׃"
    },
    "17": {
      "verse_number": 17,
      "sort_number": 17,
      "text": "[III_REG 4:17]",
      "word_count": 5,
      "hebrew": "יְהוֹשָׁפָ֥ט בֶּן פָּר֖וּחַ בְּיִשָׂשכָֽר׃"
    },
    "18": {
      "verse_number": 18,
      "sort_number": 18,
      "text": "[III_REG 4:18]",
      "word_count": 5,
      "hebrew": "שִׁמְעִ֥י בֶן אֵלָ֖א בְּבִנְיָמִֽן׃"
    },
    "19": {
      "verse_number": 19,
      "sort_number": 19,
      "text": "[III_REG 4:19]",
      "word_count": 5,
      "hebrew": "גֶּ֥בֶר בֶּן אֻרִ֖י בְּאֶ֣רֶץ גִּלְעָ֑ד אֶ֜רֶץ סִיח֣וֹן מֶ֣לֶךְ הָאֱמֹרִ֗י וְעֹג֙ מֶ֣לֶךְ הַבָּשָׁ֔ן וּנְצִ֥יב אֶחָ֖ד אֲשֶׁ֥ר בָּאָֽרֶץ׃"
    },
    "20": {
      "verse_number": 20,
      "sort_number": 20,
      "text": "[III_REG 4:20]",
      "word_count": 5,
      "hebrew": "יְהוּדָ֤ה וְיִשְׂרָאֵל֙ רַבִּ֔ים כַּח֥וֹל אֲשֶׁר עַל הַיָּ֖ם לָרֹ֑ב אֹכְלִ֥ים וְשֹׁתִ֖ים וּשְׂמֵחִֽים׃"
    },
