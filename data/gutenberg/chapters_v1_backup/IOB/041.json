  "name": "41",
  "book_key": "IOB",
  "testament": "Vetus_Testamentum",
  "section": "Libri_Sapientiae",
  "verse_count": 34,
  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[IOB 41:1]",
      "word_count": 5,
      "hebrew": "הֵן תֹּחַלְתּ֥וֹ נִכְזָ֑בָה הֲגַ֖ם אֶל מַרְאָ֣יו יֻטָֽל׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[IOB 41:2]",
      "word_count": 5,
      "hebrew": "לֹֽא אַ֭כְזָר כִּ֣י יְעוּרֶ֑נּוּ וּמִ֥י ה֝֗וּא לְפָנַ֥י יִתְיַצָּֽב׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[IOB 41:3]",
      "word_count": 5,
      "hebrew": "מִ֣י הִ֭קְדִּימַנִי וַאֲשַׁלֵּ֑ם תַּ֖חַת כָּל הַשָּׁמַ֣יִם לִי הֽוּא׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[IOB 41:4]",
      "word_count": 5,
      "hebrew": "לא לֽוֹ אַחֲרִ֥ישׁ בַּדָּ֑יו וּדְבַר גְּ֝בוּר֗וֹת וְחִ֣ין עֶרְכּֽוֹ׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[IOB 41:5]",
      "word_count": 5,
      "hebrew": "מִֽי גִ֭לָּה פְּנֵ֣י לְבוּשׁ֑וֹ בְּכֶ֥פֶל רִ֝סְנ֗וֹ מִ֣י יָבֽוֹא׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[IOB 41:6]",
      "word_count": 5,
      "hebrew": "דַּלְתֵ֣י פָ֭נָיו מִ֣י פִתֵּ֑חַ סְבִיב֖וֹת שִׁנָּ֣יו אֵימָֽה׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[IOB 41:7]",
      "word_count": 5,
      "hebrew": "גַּ֭אֲוָה אֲפִיקֵ֣י מָֽגִנִּ֑ים סָ֝ג֗וּר חוֹתָ֥ם צָֽר׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[IOB 41:8]",
      "word_count": 5,
      "hebrew": "אֶחָ֣ד בְּאֶחָ֣ד יִגַּ֑שׁוּ וְ֝ר֗וּחַ לֹא יָב֥וֹא בֵֽינֵיהֶֽם׃"
    },
    "9": {
      "verse_number": 9,
      "sort_number": 9,
      "text": "[IOB 41:9]",
      "word_count": 5,
      "hebrew": "אִישׁ בְּאָחִ֥יהוּ יְדֻבָּ֑קוּ יִ֝תְלַכְּד֗וּ וְלֹ֣א יִתְפָּרָֽדוּ׃"
    },
    "10": {
      "verse_number": 10,
      "sort_number": 10,
      "text": "[IOB 41:10]",
      "word_count": 5,
      "hebrew": "עֲֽ֭טִישֹׁתָיו תָּ֣הֶל א֑וֹר וְ֝עֵינָ֗יו כְּעַפְעַפֵּי שָֽׁחַר׃"
    },
    "11": {
      "verse_number": 11,
      "sort_number": 11,
      "text": "[IOB 41:11]",
      "word_count": 5,
      "hebrew": "מִ֭פִּיו לַפִּידִ֣ים יַהֲלֹ֑כוּ כִּיד֥וֹדֵי אֵ֝֗שׁ יִתְמַלָּֽטוּ׃"
    },
    "12": {
      "verse_number": 12,
      "sort_number": 12,
      "text": "[IOB 41:12]",
      "word_count": 5,
      "hebrew": "מִ֭נְּחִירָיו יֵצֵ֣א עָשָׁ֑ן כְּד֖וּד נָפ֣וּחַ וְאַגְמֹֽן׃"
    },
    "13": {
      "verse_number": 13,
      "sort_number": 13,
      "text": "[IOB 41:13]",
      "word_count": 5,
      "hebrew": "נַ֭פְשׁוֹ גֶּחָלִ֣ים תְּלַהֵ֑ט וְ֝לַ֗הַב מִפִּ֥יו יֵצֵֽא׃"
    },
    "14": {
      "verse_number": 14,
      "sort_number": 14,
      "text": "[IOB 41:14]",
      "word_count": 5,
      "hebrew": "בְּֽ֭צַוָּארוֹ יָלִ֣ין עֹ֑ז וּ֝לְפָנָ֗יו תָּד֥וּץ דְּאָבָֽה׃"
    },
    "15": {
      "verse_number": 15,
      "sort_number": 15,
      "text": "[IOB 41:15]",
      "word_count": 5,
      "hebrew": "מַפְּלֵ֣י בְשָׂר֣וֹ דָבֵ֑קוּ יָצ֥וּק עָ֝לָ֗יו בַּל יִמּֽוֹט׃"
    },
    "16": {
      "verse_number": 16,
      "sort_number": 16,
      "text": "[IOB 41:16]",
      "word_count": 5,
      "hebrew": "לִ֭בּוֹ יָצ֣וּק כְּמוֹ אָ֑בֶן וְ֝יָצ֗וּק כְּפֶ֣לַח תַּחְתִּֽית׃"
    },
    "17": {
      "verse_number": 17,
      "sort_number": 17,
      "text": "[IOB 41:17]",
      "word_count": 5,
      "hebrew": "מִ֭שֵּׂתוֹ יָג֣וּרוּ אֵלִ֑ים מִ֝שְּׁבָרִ֗ים יִתְחַטָּֽאוּ׃"
    },
    "18": {
      "verse_number": 18,
      "sort_number": 18,
      "text": "[IOB 41:18]",
      "word_count": 5,
      "hebrew": "מַשִּׂיגֵ֣הוּ חֶ֭רֶב בְּלִ֣י תָק֑וּם חֲנִ֖ית מַסָּ֣ע וְשִׁרְיָֽה׃"
    },
    "19": {
      "verse_number": 19,
      "sort_number": 19,
      "text": "[IOB 41:19]",
      "word_count": 5,
      "hebrew": "יַחְשֹׁ֣ב לְתֶ֣בֶן בַּרְזֶ֑ל לְעֵ֖ץ רִקָּב֣וֹן נְחוּשָֽׁה׃"
    },
    "20": {
      "verse_number": 20,
      "sort_number": 20,
      "text": "[IOB 41:20]",
      "word_count": 5,
      "hebrew": "לֹֽא יַבְרִיחֶ֥נּוּ בֶן קָ֑שֶׁת לְ֝קַ֗שׁ נֶהְפְּכוּ ל֥וֹ אַבְנֵי קָֽלַע׃"
    },
    "21": {
      "verse_number": 21,
      "sort_number": 21,
      "text": "[IOB 41:21]",
      "word_count": 5,
      "hebrew": "כְּ֭קַשׁ נֶחְשְׁב֣וּ תוֹתָ֑ח וְ֝יִשְׂחַ֗ק לְרַ֣עַשׁ כִּידֽוֹן׃"
    },
    "22": {
      "verse_number": 22,
      "sort_number": 22,
      "text": "[IOB 41:22]",
      "word_count": 5,
      "hebrew": "תַּ֭חְתָּיו חַדּ֣וּדֵי חָ֑רֶשׂ יִרְפַּ֖ד חָר֣וּץ עֲלֵי טִֽיט׃"
    },
    "23": {
      "verse_number": 23,
      "sort_number": 23,
      "text": "[IOB 41:23]",
      "word_count": 5,
      "hebrew": "יַרְתִּ֣יחַ כַּסִּ֣יר מְצוּלָ֑ה יָ֝֗ם יָשִׂ֥ים כַּמֶּרְקָחָֽה׃"
    },
    "24": {
      "verse_number": 24,
      "sort_number": 24,
      "text": "[IOB 41:24]",
      "word_count": 5,
      "hebrew": "אַ֭חֲרָיו יָאִ֣יר נָתִ֑יב יַחְשֹׁ֖ב תְּה֣וֹם לְשֵׂיבָֽה׃"
    },
    "25": {
      "verse_number": 25,
      "sort_number": 25,
      "text": "[IOB 41:25]",
      "word_count": 5,
      "hebrew": "אֵֽין עַל עָפָ֥ר מָשְׁל֑וֹ הֶ֝עָשׂ֗וּ לִבְלִי חָֽת׃"
    },
    "26": {
      "verse_number": 26,
      "sort_number": 26,
      "text": "[IOB 41:26]",
      "word_count": 5,
      "hebrew": "אֵֽת כָּל גָּבֹ֥הַּ יִרְאֶ֑ה ה֝֗וּא מֶ֣לֶךְ עַל כָּל בְּנֵי שָֽׁחַץ׃"
    },
