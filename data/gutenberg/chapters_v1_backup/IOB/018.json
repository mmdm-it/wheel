  "chapter_number": 18,
  "sort_number": 18,
  "name": "18",
  "book_key": "IOB",
  "testament": "Vetus_Testamentum",
  "section": "Libri_Sapientiae",
  "verse_count": 21,
  "verses": {
    "1": {
      "verse_number": 1,
      "sort_number": 1,
      "text": "[IOB 18:1]",
      "word_count": 5,
      "hebrew": "וַ֭יַּעַן בִּלְדַּ֥ד הַשֻּׁחִ֗י וַיֹּאמַֽר׃"
    },
    "2": {
      "verse_number": 2,
      "sort_number": 2,
      "text": "[IOB 18:2]",
      "word_count": 5,
      "hebrew": "עַד אָ֤נָה תְּשִׂימ֣וּן קִנְצֵ֣י לְמִלִּ֑ין תָּ֝בִ֗ינוּ וְאַחַ֥ר נְדַבֵּֽר׃"
    },
    "3": {
      "verse_number": 3,
      "sort_number": 3,
      "text": "[IOB 18:3]",
      "word_count": 5,
      "hebrew": "מַ֭דּוּעַ נֶחְשַׁ֣בְנוּ כַבְּהֵמָ֑ה נִ֝טְמִ֗ינוּ בְּעֵינֵיכֶֽם׃"
    },
    "4": {
      "verse_number": 4,
      "sort_number": 4,
      "text": "[IOB 18:4]",
      "word_count": 5,
      "hebrew": "טֹֽרֵ֥ף נַפְשׁ֗וֹ בְּאַ֫פּ֥וֹ הַ֭לְמַעַנְךָ תֵּעָ֣זַב אָ֑רֶץ וְיֶעְתַּק צ֝֗וּר מִמְּקֹמֽוֹ׃"
    },
    "5": {
      "verse_number": 5,
      "sort_number": 5,
      "text": "[IOB 18:5]",
      "word_count": 5,
      "hebrew": "גַּ֤ם א֣וֹר רְשָׁעִ֣ים יִדְעָ֑ךְ וְלֹֽא יִ֝גַּ֗הּ שְׁבִ֣יב אִשּֽׁוֹ׃"
    },
    "6": {
      "verse_number": 6,
      "sort_number": 6,
      "text": "[IOB 18:6]",
      "word_count": 5,
      "hebrew": "א֭וֹר חָשַׁ֣ךְ בְּאָהֳל֑וֹ וְ֝נֵר֗וֹ עָלָ֥יו יִדְעָֽךְ׃"
    },
    "7": {
      "verse_number": 7,
      "sort_number": 7,
      "text": "[IOB 18:7]",
      "word_count": 5,
      "hebrew": "יֵֽ֭צְרוּ צַעֲדֵ֣י אוֹנ֑וֹ וְֽתַשְׁלִיכֵ֥הוּ עֲצָתֽוֹ׃"
    },
    "8": {
      "verse_number": 8,
      "sort_number": 8,
      "text": "[IOB 18:8]",
      "word_count": 5,
      "hebrew": "כִּֽי שֻׁלַּ֣ח בְּרֶ֣שֶׁת בְּרַגְלָ֑יו וְעַל שְׂ֝בָכָ֗ה יִתְהַלָּֽךְ׃"
    },
    "9": {
      "verse_number": 9,
      "sort_number": 9,
      "text": "[IOB 18:9]",
      "word_count": 5,
      "hebrew": "יֹאחֵ֣ז בְּעָקֵ֣ב פָּ֑ח יַחֲזֵ֖ק עָלָ֣יו צַמִּֽים׃"
    },
    "10": {
      "verse_number": 10,
      "sort_number": 10,
      "text": "[IOB 18:10]",
      "word_count": 5,
      "hebrew": "טָמ֣וּן בָּאָ֣רֶץ חַבְל֑וֹ וּ֝מַלְכֻּדְתּ֗וֹ עֲלֵ֣י נָתִֽיב׃"
    },
    "11": {
      "verse_number": 11,
      "sort_number": 11,
      "text": "[IOB 18:11]",
      "word_count": 5,
      "hebrew": "סָ֭בִיב בִּֽעֲתֻ֣הוּ בַלָּה֑וֹת וֶהֱפִיצֻ֥הוּ לְרַגְלָֽיו׃"
    },
    "12": {
      "verse_number": 12,
      "sort_number": 12,
      "text": "[IOB 18:12]",
      "word_count": 5,
      "hebrew": "יְהִי רָעֵ֥ב אֹנ֑וֹ וְ֝אֵ֗יד נָכ֥וֹן לְצַלְעֽוֹ׃"
    },
    "13": {
      "verse_number": 13,
      "sort_number": 13,
      "text": "[IOB 18:13]",
      "word_count": 5,
      "hebrew": "יֹ֭אכַל בַּדֵּ֣י עוֹר֑וֹ יֹאכַ֥ל בַּ֝דָּ֗יו בְּכ֣וֹר מָֽוֶת׃"
    },
    "14": {
      "verse_number": 14,
      "sort_number": 14,
      "text": "[IOB 18:14]",
      "word_count": 5,
      "hebrew": "יִנָּתֵ֣ק מֵ֭אָהֳלוֹ מִבְטַח֑וֹ וְ֝תַצְעִדֵ֗הוּ לְמֶ֣לֶךְ בַּלָּהֽוֹת׃"
    },
    "15": {
      "verse_number": 15,
      "sort_number": 15,
      "text": "[IOB 18:15]",
      "word_count": 5,
      "hebrew": "תִּשְׁכּ֣וֹן בְּ֭אָהֳלוֹ מִבְּלִי ל֑וֹ יְזֹרֶ֖ה עַל נָוֵ֣הוּ גָפְרִֽית׃"
    },
    "16": {
      "verse_number": 16,
      "sort_number": 16,
      "text": "[IOB 18:16]",
      "word_count": 5,
      "hebrew": "מִ֭תַּחַת שָֽׁרָשָׁ֣יו יִבָ֑שׁוּ וּ֝מִמַּ֗עַל יִמַּ֥ל קְצִירֽוֹ׃"
    },
    "17": {
      "verse_number": 17,
      "sort_number": 17,
      "text": "[IOB 18:17]",
      "word_count": 5,
      "hebrew": "זִֽכְרוֹ אָ֭בַד מִנִּי אָ֑רֶץ וְלֹא שֵׁ֥ם ל֝֗וֹ עַל פְּנֵי חֽוּץ׃"
    },
    "18": {
      "verse_number": 18,
      "sort_number": 18,
      "text": "[IOB 18:18]",
      "word_count": 5,
      "hebrew": "יֶ֭הְדְּפֻהוּ מֵא֣וֹר אֶל חֹ֑שֶׁךְ וּֽמִתֵּבֵ֥ל יְנִדֻּֽהוּ׃"
    },
    "19": {
      "verse_number": 19,
      "sort_number": 19,
      "text": "[IOB 18:19]",
      "word_count": 5,
      "hebrew": "לֹ֘א נִ֤ין ל֣וֹ וְלֹא נֶ֣כֶד בְּעַמּ֑וֹ וְאֵ֥ין שָׂ֝רִ֗יד בִּמְגוּרָֽיו׃"
    },
    "20": {
      "verse_number": 20,
      "sort_number": 20,
      "text": "[IOB 18:20]",
      "word_count": 5,
      "hebrew": "עַל י֭וֹמוֹ נָשַׁ֣מּוּ אַחֲרֹנִ֑ים וְ֝קַדְמֹנִ֗ים אָ֣חֲזוּ שָֽׂעַר׃"
    },
    "21": {
      "verse_number": 21,
      "sort_number": 21,
      "text": "[IOB 18:21]",
      "word_count": 5,
      "hebrew": "אַךְ אֵ֭לֶּה מִשְׁכְּנ֣וֹת עַוָּ֑ל וְ֝זֶ֗ה מְק֣וֹם לֹא יָדַֽע אֵֽל׃"
    }
  }
}