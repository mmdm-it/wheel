        "testaments": {
            "Vetus_Testamentum": {
                "sections": {
                    "Pentateuchus": {
                        "books": {
                            "Liber_Genesis": {
                                "book_number": 1,
                                "sort_number": 1,
                                "chapters": {
                                    "1": {
                                        "verses": {
                                            "1": {
                                                "text": "In principio creavit Deus caelum et terram.",
                                                "translation": "In the beginning God created the heavens and the earth."
                                            },
                                            "2": {
                                                "text": "Terra autem erat inanis et vacua, et tenebrae erant super faciem abyssi; et Spiritus Dei ferebatur super aquas.",
                                                "translation": "And the earth was without form and void, and darkness was upon the face of the deep; and the Spirit of God moved upon the face of the waters."
                                            },
                                            "3": {
                                                "text": "Dixitque Deus: 'Fiat lux.' Et facta est lux.",
                                                "translation": "And God said: Let there be light. And there was light."
                                            },
                                            "4": {
                                                "text": "Et vidit Deus lucem quod esset bona; et divisit lucem a tenebris.",
                                                "translation": "And God saw the light that it was good; and He divided the light from the darkness."
                                            },
                                            "5": {
                                                "text": "Appellavitque lucem diem, et tenebras noctem; factumque est vespere et mane, dies unus.",
                                                "translation": "And He called the light Day, and the darkness He called Night; and there was evening and morning, one day."
                                            }
                                        }
                                    },
                                    "2": {
                                        "verses": {
                                            "1": {
                                                "text": "Igitur perfecti sunt caeli et terra, et omnis ornatus eorum.",
                                                "translation": "So the heavens and the earth were finished, and all the furniture of them."
                                            },
                                            "2": {
                                                "text": "Complevitque Deus die septimo opus suum quod fecerat; et requievit die septimo ab universo opere quod patrarat.",
                                                "translation": "And on the seventh day God ended his work which he had made: and he rested on the seventh day from all his work which he had done."
                                            },
                                            "3": {
                                                "text": "Et benedixit diei septimo, et sanctificavit illum, quia in ipso cessaverat ab omni opere suo quod creavit Deus ut faceret.",
                                                "translation": "And he blessed the seventh day, and sanctified it: because in it he had rested from all his work which God created and made."
                                            }
                                        }
                                    },
                                    "3": {
                                        "verses": {
                                            "1": {
                                                "text": "Sed et serpens erat callidior cunctis animantibus terrae quae fecerat Dominus Deus.",
                                                "translation": "Now the serpent was more subtle than any of the beasts of the earth which the Lord God had made."
                                            }
                                        }
                                    }
                                }
                            },
                            "Liber_Exodus": {
                                "book_number": 2,
                                "sort_number": 2,
                                "chapters": {
                                    "1": {
                                        "verses": {
                                            "1": {
                                                "text": "Haec sunt nomina filiorum Israel qui ingressi sunt in Aegyptum cum Iacob; singuli cum domibus suis introierunt.",
                                                "translation": "These are the names of the children of Israel, that went into Egypt with Jacob: they went in, every man with his household."
                                            }
                                        }
                                    }
                                }
                            },
                            "Liber_Leviticus": {
                                "book_number": 3,
                                "sort_number": 3,
                                "chapters": {
                                    "1": {
                                        "verses": {
                                            "1": {
                                                "text": "Vocavit autem Moysen, et locutus est ei Dominus de tabernaculo testimonii, dicens.",
                                                "translation": "And the Lord called Moses, and spoke to him from the tabernacle of the testimony, saying."
                                            }
                                        }
                                    }
                                }
                            },
                            "Liber_Numeri": {
                                "book_number": 4,
                                "sort_number": 4,
                                "chapters": {
                                    "1": {
                                        "verses": {
                                            "1": {
                                                "text": "Locutusque est Dominus ad Moysen in deserto Sinai in tabernaculo foederis, prima die mensis secundi, anno altero egressionis eorum ex Aegypto, dicens.",
                                                "translation": "And the Lord spoke to Moses in the desert of Sinai in the tabernacle of the covenant, the first day of the second month, the second year of their going out of Egypt, saying."
                                            }
                                        }
                                    }
                                }
                            },
                            "Liber_Deuteronomii": {
                                "book_number": 5,
                                "sort_number": 5,
                                "chapters": {
                                    "1": {
                                        "verses": {
                                            "1": {
                                                "text": "Haec sunt verba quae locutus est Moyses ad omnem Israel trans Iordanem in solitudine campestri, contra Mare Rubrum, inter Pharan et Tophel et Laban et Haseroth, ubi auri est plurimum.",
                                                "translation": "These are the words, which Moses spoke to all Israel beyond the Jordan, in the plain wilderness, over against the Red Sea, between Pharan and Tophel and Laban and Haseroth, where there is very much gold."
                                            }
                                        }
                                    }
                                }
                            }
                        }
